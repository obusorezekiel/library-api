{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/api/library",
            "dest": "index.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/library",
            "dest": "index.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/api/library/id",
            "dest": "index.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/library/id",
            "dest": "index.js",
            "methods": [
                "PATCH"
            ]
        },
        {
            "src": "/api/library/id",
            "dest": "index.js",
            "methods": [
                "DELETE"
            ]
        }
    ]
}